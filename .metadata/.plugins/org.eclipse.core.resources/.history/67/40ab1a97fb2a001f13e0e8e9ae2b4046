.section .data

SIGN_DATA:
.byte +13
.byte -10
.byte +9
.byte +14
.byte -18
.byte -9
.byte +12
.byte -19
.byte +16





.syntax unified
.cpu cortex-m0
.fpu softvfp
.thumb
.section .text
.global __main

__main:
	//load data address in r0//
	ldr r0,=SIGN_DATA
	movs r3,#9


stop:
	b	stop
	.align
	.end
